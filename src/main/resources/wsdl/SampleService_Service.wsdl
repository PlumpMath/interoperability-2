<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="SampleService" targetNamespace="http://www.wtf.com/interoperability/1.0" xmlns:ns1="http://services.interoperability.wtf.org/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://www.wtf.com/interoperability/1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/">
  <wsdl:import namespace="http://services.interoperability.wtf.org/" location="SampleService.wsdl">
    </wsdl:import>
  <wsdl:binding name="SampleServiceSoapBinding" type="ns1:SampleService">
    <soap12:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="getShop">
      <soap12:operation soapAction="" style="document"/>
      <wsdl:input name="getShop">
        <soap12:body use="literal"/>
      </wsdl:input>
      <wsdl:output name="getShopResponse">
        <soap12:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="SampleService">
    <wsdl:port name="SampleServicePort" binding="tns:SampleServiceSoapBinding">
      <soap12:address location="http://localhost:9090/SampleServicePort"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
